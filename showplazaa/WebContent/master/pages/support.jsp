<%@ taglib uri="/struts-tags" prefix="s" %>

<section>
      <div class="container-fluid banner" style="background-image: url(img/contact-us-banner.jpg);">
        <div class="container-fluid inner-banner">
          <div class="inner-banner-left text-center">
            <h1>Contact Us</h1>
            <p>Get in Touch with us</p>
          </div>
        </div>
      </div>
    </section>
    <section class="contact-section">
        <div class="row">
          <div class="container-fluid">
            <div class="col-sm-7 col-xs-12 margin20">
                <h3>Write To Us</h3>
                <form class="form">
                    <div class="form-group col-xs-12">
                        <select class="form-control">
                            <option>For Customer Queries</option>
                            <option>For Business enquiries</option>
                            <option>For Media/Press enquiries</option>
                            <option>I want to Host mymy event on Showplazza</option>
                            <option>Other enquiries</option>
                        </select>
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="text" class="form-control" placeholder="Organizer Name (Required)" required>
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="text" class="form-control" placeholder="Company Name(Required)" required>
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="text" class="form-control" placeholder="Region Name" required>
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="text" class="form-control" placeholder="Event Genre" required>
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="number" class="form-control" placeholder="Inventory to Sell (Ticket Count)" required>
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="email" class="form-control" placeholder="Email ID" required>
                    </div>
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="text" class="form-control" placeholder="Mobile No." required>
                    </div> 
                    <div class="form-group col-sm-6 col-xs-12">
                        <input type="text" class="form-control" placeholder="Event Remark" required>
                    </div>
                    <div class="col-xs-12 no-padding">
                        <button class="btn sub-btn">SUBMIT</button>
                    </div>
                </form>
            </div>
            <div class="col-sm-5 col-xs-12 margin20">
                <h3>Contact Us</h3>
                <div class="contact-wrap">
                    <p class="font-sz-16 clearfix"><span class="bms-icnsigns2"></span><span>Plot no 39 B , Block no 202, 2nd floor Prabhakar Smruti Appartment, Khamla Road, Pandy layout Nagpur - 440025 </span></p>
                    <p class="font-sz-16 clearfix"><span class="bms-icnnotification-bell"></span><span>+919028420471</span></p>
                    <p class="font-sz-16 clearfix"><span class="bms-icntechnology"></span><span>info@showplazza.com</span></p>
                     <p class="font-sz-16 clearfix"><span class="bms-icntechnology"></span><span>marketing@showplazza.com</span></p>
                      <p class="font-sz-16 clearfix"><span class="bms-icntechnology"></span><span>support@showplazza.com</span></p>
                </div>
            </div>
          </div>
        </div>
    </section>